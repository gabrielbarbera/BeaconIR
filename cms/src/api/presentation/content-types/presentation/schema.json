{
  "kind": "collectionType",
  "collectionName": "presentations",
  "info": {
    "singularName": "presentation",
    "pluralName": "presentations",
    "displayName": "Presentation"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "date": {
      "type": "date",
      "required": true
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "Investor Presentation",
        "Earnings Call",
        "Conference",
        "Annual Report"
      ],
      "required": true
    },
    "document": {
      "type": "media",
      "multiple": false,
      "required": true
    },
    "description": {
      "type": "text"
    },
    "company": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::company.company"
    }
  }
}

